                                         pubkCard, CVM
     ),
     In( <nc, SDAD> )
     ]
    --[ Eq( z, true ) ]->
     [
     Terminal_Ready_For_CVM( $Terminal, ~PAN, $Bank, $CA,
                             <$amount, country, currency, date, type, ~UN>, AIP, pubkBank, pubkCard,
                             CVM
     )
     ]
    variants (modulo AC)
    1. ~UN   = ~UN.18
