rule (modulo E) Create_Card_Visa:
   [
   Fr( ~PAN ), Fr( ~expDate ), Fr( ~privkCard ), Fr( ~MK ),
   !LtkBank( $Bank, ~privkBank ), !CertBank( $Bank, certBank ),
   !IssuingCA( $Bank, $CA )
   ]
